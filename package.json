{
  "name": "project-showcase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:static": "yarn clean && next build",
    "clean": "rm -rf .next build",
    "dev": "next dev -p 3001",
    "lint": "next lint",
    "lint:md": "eslint public/**/*.mdx",
